<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="images/IGRP/IGRP2.3/app/agenda/agdashboard/AGDashboard.xsl" type="text/xsl"?>
<rows>
<link_img>/IGRP/images/IGRP/IGRP2.3</link_img>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>AGDashboard</page>
    <app>agenda</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.agenda.pages</package_db>
        <package_html>AGDashboard</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>http://localhost:8080/IGRP/webapps?r=igrp/page/preserveUrl&amp;p_id=</preserve_url>
    </plsql>
    <content>
        <title>Dashboard Agenda Eletronica</title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <entidade name="p_entidade" type="lookup" action="" page="" app="" lookup_type="LOOKUP" class="primary" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Entidade</label>
                    <value/>
                    <lookup>http://igrp.teste.gov.cv/images/IGRP.bootstrap/app/BOOTSTRAP/xml/lookup.test.xml</lookup>
                </entidade>
                <data_de_ name="p_data_de_" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="primary">
                    <label>Data de </label>
                    <value/>
                </data_de_>
                <data_ate name="p_data_ate" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="primary">
                    <label>Data até</label>
                    <value/>
                </data_ate>
            </fields>
            <tools-bar>
                <item type="form" code="" class="primary" rel="pesquisar">
                    <title>Pesquisar</title>
                    <app>agenda</app>
                    <page>AddServicos</page>
                    <link>index</link>
                    <parameter/>
                    <target>submit</target>
                    <img>primary|fa-search</img>
                </item>
            </tools-bar>
        </form_1>
        <box_1 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_1>
        <agendamentos_do_dia type="statbox" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <agendamentos_do_dia_tit name="p_agendamentos_do_dia_tit" type="text" maxlength="4000" persist="true">
                    <label>Box Title</label>
                    <value>Box Title</value>
                </agendamentos_do_dia_tit>
                <agendamentos_do_dia_val name="p_agendamentos_do_dia_val" type="text" maxlength="4000" persist="true">
                    <label>Value</label>
                    <value>895</value>
                </agendamentos_do_dia_val>
                <agendamentos_do_dia_txt name="p_agendamentos_do_dia_txt" type="text" maxlength="4000" persist="true">
                    <label>Url Text</label>
                    <value>Read More...</value>
                </agendamentos_do_dia_txt>
                <agendamentos_do_dia_url name="p_agendamentos_do_dia_url" type="text" maxlength="4000" persist="true">
                    <label>Url</label>
                    <value>http://www.example.com</value>
                </agendamentos_do_dia_url>
                <agendamentos_do_dia_bg name="p_agendamentos_do_dia_bg" type="text" maxlength="4000" persist="true">
                    <label>Background</label>
                    <value>cp-yellow</value>
                </agendamentos_do_dia_bg>
                <agendamentos_do_dia_icn name="p_agendamentos_do_dia_icn" type="text" maxlength="4000" persist="true">
                    <label>Icon</label>
                    <value>fa-calendar-check-o</value>
                </agendamentos_do_dia_icn>
            </fields>
        </agendamentos_do_dia>
        <chart_1 type="chart" xml-type="chart" title="Agendamentos por balcão" gen-type="container" gen-group="">
            <caption/>
            <chart_type>column</chart_type>
            <xaxys>Eixo de X</xaxys>
            <yaxys>Eixo de Y</yaxys>
            <url>#</url>
            <label>
                <col>Ano</col>
                <col>X1</col>
                <col>X2</col>
                <col>X3</col>
                <col>X4</col>
            </label>
            <value>
                <row>
                    <col>2010</col>
                    <col>265</col>
                    <col>658</col>
                    <col>498</col>
                    <col>698</col>
                </row>
                <row>
                    <col>2009</col>
                    <col>784</col>
                    <col>258</col>
                    <col>594</col>
                    <col>498</col>
                </row>
                <row>
                    <col>2015</col>
                    <col>1010</col>
                    <col>698</col>
                    <col>366</col>
                    <col>498</col>
                </row>
            </value>
            <colors>
                <col>#cc122e</col>
                <col>#ec0f9d</col>
                <col>#5317d9</col>
                <col>#7c8ea2</col>
            </colors>
        </chart_1>
        <agendamentos_atendidos type="circlestatbox" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <agendamentos_atendidos_tit name="p_agendamentos_atendidos_tit" type="text" maxlength="4000" persist="true">
                    <label>Circle Box Title</label>
                    <value>Circle Box Title</value>
                </agendamentos_atendidos_tit>
                <agendamentos_atendidos_lbl name="p_agendamentos_atendidos_lbl" type="text" maxlength="4000" persist="true">
                    <label>Percent Text</label>
                    <value>percent</value>
                </agendamentos_atendidos_lbl>
                <agendamentos_atendidos_val name="p_agendamentos_atendidos_val" type="text" maxlength="4000">
                    <label>Percent Value</label>
                    <value>58.5</value>
                </agendamentos_atendidos_val>
                <agendamentos_atendidos_desc name="p_agendamentos_atendidos_desc" type="text" maxlength="4000" persist="true">
                    <label>Description</label>
                    <value>Some Text Here</value>
                </agendamentos_atendidos_desc>
                <agendamentos_atendidos_url name="p_agendamentos_atendidos_url" type="text" maxlength="4000">
                    <label>Link</label>
                    <value>http://www.example.com</value>
                </agendamentos_atendidos_url>
                <agendamentos_atendidos_bg name="p_agendamentos_atendidos_bg" type="text" maxlength="4000" persist="true">
                    <label>Background</label>
                    <value>cp-teal</value>
                </agendamentos_atendidos_bg>
            </fields>
        </agendamentos_atendidos>
        <box_2 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_2>
